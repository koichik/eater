{
  "name": "eater",
  "version": "1.5.0",
  "description": "eater is EAsy Test runnER",
  "main": "index.js",
  "bin": "./bin/eater.js",
  "scripts": {
    "test": "npm run test:core && npm run test:runner",
    "test:core": "./bin/eater.js --dir test/core",
    "test:runner": "./bin/eater.js --dir test/runner",
    "test:babel": "./bin/eater.js --dir test/babel",
    "cov": "nyc npm test",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "test",
    "assert"
  ],
  "author": "yosuke-furukawa",
  "license": "MIT",
  "dependencies": {
    "colo": "^0.3.1",
    "exists-sync": "0.0.3",
    "json5": "^0.5.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "coveralls": "^2.11.9",
    "espower-babel": "^4.0.3",
    "espower-loader": "^1.0.0",
    "mock-require": "^1.2.1",
    "nyc": "^6.4.0",
    "power-assert": "^1.3.1",
    "must-call": "^1.0.0"
  }
}
